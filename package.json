{
  "name": "@ombox/mdhub",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:oh-my-box/mdhub.git",
  "author": "lencx <cxin1314@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "esno ./src/index.ts",
    "build": "tsup --dts --minify --format cjs,esm"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "dependencies": {
    "highlight.js": "^11.5.1",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.0.0",
    "esno": "^0.16.3",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  }
}
